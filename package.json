{
  "name": "houter",
  "version": "1.0.10",
  "description": "houter React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es"
  ],
  "scripts": {
    "build": "rollup -c ./config/rollup.config.js",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "jest --config ./config/jest.config.js --verbose --coverage --no-cache",
    "test:watch": "jest --config ./config/jest.config.js --watchAll --verbose --coverage --no-cache",
    "lint": "eslint src/* stories/*",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "history": "^5.3.0",
    "path-to-regexp": "^3.0.0"
  },
  "peerDependencies": {
    "react": "16.8.x"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/npm": "^5.1.15",
    "@storybook/addon-info": "^5.1.11",
    "@storybook/addon-notes": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/enzyme": "^3.10.3",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.16",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.3",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-docgen-typescript-loader": "^3.1.1",
    "react-dom": "^16.8.6",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.22.1",
    "semantic-release": "^15.13.24",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "author": "LAM",
  "license": "MIT",
  "repository": "https://github.com/lintuming/houter",
  "keywords": [
    "react-component"
  ]
}
